{% extends "layout/layout.njk" %}
{% block content %}
<div class="page-header">
  <div class="container">
    <h1>{{data.site.name}} <small>Templates Overview</small></h1>
  </div>
</div>
<div class="container">
  <div class="list-group">
    {% for view in data.views %}
      <a href="{{view.url}}" class="list-group-item">
        {{view.name}}
      </a>
    {% endfor %}
  </div>
</div>
{% endblock %}
